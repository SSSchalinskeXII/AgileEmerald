<div class="container">
  <div class="well">
    <%= link_to "New Score", new_score_path, remote: true, class: "btn btn-primary" %>
  </div>
  <div class="new-score"></div>

  <table class="table table-bordered table-striped">
    <thead>
      <tr>
        <th>Email</th>
        <th>Score</th>
        <td>&nbsp;</td>
        <td>&nbsp;</td>
      </tr>
    </thead>
    <tbody class="score-index">
      <%= render "index" %>
    </tbody>
  </table>
</div>
<div id="score-modal" class="modal fade"></div>
